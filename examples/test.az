
struct object
{
    val: i64;

    fn drop(self: &object) -> null
    {
        println("dropping object");
    }
}

struct outer
{
    inner: object;

    fn drop(self: &outer) -> null
    {
        println("dropping outer");
    }
}

{
    println("before creation");
    x := outer(object(5));
    println("after creation");
}

