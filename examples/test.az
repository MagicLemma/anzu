
struct vec2
{
    x: int
    y: int
}

struct span
{
    p: &vec2
    size: int
}

fn print_list(s: span) -> null
{
    idx := 0
    while idx != s.size {
        println(*(s.p + idx))
        idx = idx + 1
    }
    *(s.p + 1) = vec2(10, 20)
}

l := [vec2(1, 2), vec2(3, 4), vec2(5, 6)]

my_span := span(&l[0], 3)

print_list(my_span)
println(l)

