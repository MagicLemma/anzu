struct copyable
{
    i: i64;
    f: f64;

    fn assign(self: copyable&, other: copyable&) -> null
    {
        other@.i = self@.i;
        other@.f = self@.f;
    }

    fn copy(self: copyable&) -> copyable
    {
        return copyable(self@.i, self@.f);
    }
}

i := 5;
ir := ref i;

c := copyable(ir, 6.0);
println(c.i);