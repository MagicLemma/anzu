struct copyable
{
    arr: i64[2u];

    fn assign(self: copyable&, other: copyable&) -> null
    {
        other@.arr = self@.arr;
    }

    fn copy(self: copyable&) -> copyable
    {
        return copyable(self@.arr);
    }
}

a := [1, 2];
ar := ref a;

c := copyable(a);
a[0u] = 3;

println(c.arr[0u]);
println(c.arr[1u]);