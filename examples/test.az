
struct object
{
    id: i64;

    fn drop(self: &object) -> null
    {
        print("dropping object ");
        println(self->id);
    }
}

{
    println("before creation");
    x := [object(1), object(2)];
    println(x[0u].id);
    println(x[1u].id);
    println("after creation");
}

println("end");