struct vec2
{
    x: i64
    y: i64
}

fn println(v: vec2) -> null
{
    print("vec2 = {")
    print(v.x)
    print(", ")
    print(v.y)
    println("}")
}

l := [vec2(1, 2), vec2(3, 4), vec2(5, 6)]

p := &l[0u]
size := size_of(l) / size_of(l[0])
idx := 0u

while idx != size {
    println(*(p + idx))
    println(idx)
    idx = idx + 1u
}

println(*(p + 0u))
println(*(p + 1u))
println(*(p + 2u))