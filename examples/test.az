struct range_iter
{
    _curr: i64;
    _max: i64;

    fn valid(self: const&) -> bool
    {
        return self._curr < self._max;
    }

    fn next(self: &) -> i64
    {
        let ret := self._curr;
        self._curr = self._curr + 1;
        return ret;
    }
}

fn range(max: i64) -> range_iter
{
    return range_iter(0, max);
}

for x in range(10) {
    print("{}\n", x);
}