
struct vec3
{
    x: f64;
    y: f64;
    z: f64;

    fn length!(T)(self: vec3 const&) -> f64
    {
        print("size of T = {}\n", sizeof(T));
        return self.x + self.y + self.z;
    }
}

var v := vec3(3.0, 4.0, 5.0);
var p := v&;
print("length of vec3(3.0, 4.0, 5.0): {}\n", p.length!(u64)());