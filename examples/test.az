

fn get(a: arena&, count: u64) -> u64[]
{
    var p := a@.create_array<u64>(count);
    var idx := 0u;
    while (idx < count) {
        p[idx] = idx;
        idx = idx + 1u;
    }
    return p;
}

arena a;
var z := get(a&, 20u);

var idx := 0u;
while (idx < z.size()) {
    print("{}\n", z[idx])
    idx = idx + 1u;
}