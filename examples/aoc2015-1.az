idx := 0u;
size := size_of(data) / size_of(data[0]);
count := 0;

while idx < size {
    c := data[idx];
    if c == '(' {
        count = count + 1;
    }
    else if c == ')' {
        count = count - 1;
    }
    idx = idx + 1u;
}

